<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:id="@+id/scroll_container_calendar"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fillViewport="true"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent">

        <LinearLayout
            android:id="@+id/container_calendar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/container_login"
            android:orientation="vertical"
            android:gravity="left|top">

            <LinearLayout
                android:id="@+id/container_calendar1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/container_login_2"
                android:gravity="left|top"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/container_depth_frame1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clipToPadding="false"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingLeft="16dp"
                    android:paddingTop="16dp"
                    android:paddingRight="16dp"
                    android:paddingBottom="8dp">

                    <ImageView
                        android:id="@+id/btn_back"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:scaleType="centerInside"
                        android:src="@drawable/ic_arrow_left"
                        android:clickable="true"
                        android:focusable="true"
                        android:background="?attr/selectableItemBackgroundBorderless"/>

                    <TextView
                        android:id="@+id/container_depth_frame3"
                        android:layout_width="match_parent"
                        android:layout_height="23dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:includeFontPadding="false"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:text="Habit Tracker"
                        android:textColor="#FFFFFFFF"
                        android:textSize="18dp" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/container_depth_frame10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginRight="16dp"
                    android:clipToPadding="false"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingLeft="4dp"
                    android:paddingTop="4dp"
                    android:paddingRight="4dp"
                    android:paddingBottom="4dp">

                    <ImageView
                        android:id="@+id/container_depth_frame12"
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:layout_marginTop="11dp"
                        android:layout_marginBottom="11dp"
                        android:scaleType="centerInside"
                        android:src="@drawable/ic_arrow_left" />

                    <TextView
                        android:id="@+id/container_depth_frame14"
                        android:layout_width="match_parent"
                        android:layout_height="21dp"
                        android:layout_weight="1"
                        android:gravity="center_horizontal|center_vertical"
                        android:includeFontPadding="false"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:text="October 2024"
                        android:textColor="#FFFFFFFF"
                        android:textSize="16dp" />

                    <ImageView
                        android:id="@+id/container_depth_frame16"
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:layout_marginTop="11dp"
                        android:layout_marginBottom="11dp"
                        android:scaleType="centerInside"
                        android:src="@drawable/ic_chevron_right" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/container_depth_frame19"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="2dp"
                    android:layout_marginRight="16dp"
                    android:clipToPadding="false"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/container_depth_frame20"
                        android:layout_width="0dp"
                        android:layout_height="48dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:includeFontPadding="false"
                        android:text="S"
                        android:textColor="#FFFFFFFF"
                        android:textSize="13dp" />

                    <TextView
                        android:id="@+id/container_depth_frame21"
                        android:layout_width="0dp"
                        android:layout_height="48dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:includeFontPadding="false"
                        android:text="M"
                        android:textColor="#FFFFFFFF"
                        android:textSize="13dp" />

                    <TextView
                        android:id="@+id/container_depth_frame22"
                        android:layout_width="0dp"
                        android:layout_height="48dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:includeFontPadding="false"
                        android:text="T"
                        android:textColor="#FFFFFFFF"
                        android:textSize="13dp" />

                    <TextView
                        android:id="@+id/container_depth_frame23"
                        android:layout_width="0dp"
                        android:layout_height="48dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:includeFontPadding="false"
                        android:text="W"
                        android:textColor="#FFFFFFFF"
                        android:textSize="13dp" />

                    <TextView
                        android:id="@+id/container_depth_frame24"
                        android:layout_width="0dp"
                        android:layout_height="48dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:includeFontPadding="false"
                        android:text="T"
                        android:textColor="#FFFFFFFF"
                        android:textSize="13dp" />

                    <TextView
                        android:id="@+id/container_depth_frame25"
                        android:layout_width="0dp"
                        android:layout_height="48dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:includeFontPadding="false"
                        android:text="F"
                        android:textColor="#FFFFFFFF"
                        android:textSize="13dp" />

                    <TextView
                        android:id="@+id/container_depth_frame26"
                        android:layout_width="0dp"
                        android:layout_height="48dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:includeFontPadding="false"
                        android:text="S"
                        android:textColor="#FFFFFFFF"
                        android:textSize="13dp" />
                </LinearLayout>



                <TextView
                    android:id="@+id/container_depth_frame92"
                    android:layout_width="match_parent"
                    android:layout_height="23dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginRight="16dp"
                    android:layout_marginBottom="8dp"
                    android:gravity="left|center_vertical"
                    android:includeFontPadding="false"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:text="Habit Completion"
                    android:textColor="#FFFFFFFF"
                    android:textSize="18dp" />

                <LinearLayout
                    android:id="@+id/container_depth_frame93"
                    android:layout_width="match_parent"
                    android:layout_height="56dp"
                    android:clipToPadding="false"
                    android:gravity="left|center_vertical"
                    android:orientation="horizontal"
                    android:paddingLeft="16dp"
                    android:paddingRight="16dp">

                    <LinearLayout
                        android:id="@+id/container_depth_frame95"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:background="@drawable/container_navi"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/container_depth_frame96"
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:scaleType="centerInside"
                            android:src="@drawable/ic_circle_check" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/container_depth_frame97"
                        android:layout_width="match_parent"
                        android:layout_height="24dp"
                        android:layout_marginLeft="16dp"
                        android:layout_weight="1"
                        android:ellipsize="end"
                        android:gravity="left|center_vertical"
                        android:includeFontPadding="false"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:text="Exercise"
                        android:textColor="#FFFFFFFF"
                        android:textSize="16dp" />

                    <TextView
                        android:id="@+id/container_depth_frame99"
                        android:layout_width="match_parent"
                        android:layout_height="24dp"
                        android:layout_weight="1"
                        android:gravity="left|center_vertical"
                        android:includeFontPadding="false"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:text="Completed"
                        android:textColor="#FFFFFFFF"
                        android:textSize="16dp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/container_depth_frame100"
                    android:layout_width="match_parent"
                    android:layout_height="56dp"
                    android:clipToPadding="false"
                    android:gravity="left|center_vertical"
                    android:orientation="horizontal"
                    android:paddingLeft="16dp"
                    android:paddingRight="16dp">

                    <LinearLayout
                        android:id="@+id/container_depth_frame102"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:background="@drawable/container_navi"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/container_depth_frame103"
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:scaleType="centerInside"
                            android:src="@drawable/ic_close_circle" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/container_depth_frame104"
                        android:layout_width="match_parent"
                        android:layout_height="24dp"
                        android:layout_marginLeft="16dp"
                        android:layout_weight="1"
                        android:ellipsize="end"
                        android:gravity="left|center_vertical"
                        android:includeFontPadding="false"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:text="Read"
                        android:textColor="#FFFFFFFF"
                        android:textSize="16dp" />

                    <TextView
                        android:id="@+id/container_depth_frame106"
                        android:layout_width="match_parent"
                        android:layout_height="24dp"
                        android:layout_weight="1"
                        android:gravity="left|center_vertical"
                        android:includeFontPadding="false"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:text="Missed"
                        android:textColor="#FFFFFFFF"
                        android:textSize="16dp" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>